def password(n):
    pa=string.ascii_letters+string.digits+string.punctuation
    return ''.join(random.choice(pa) for i in range(n))
def username(n):
    use=string.ascii_lowercase+string.ascii_uppercase+string.digits
    return ''.join(random.choice(use) for i in range(n))
s=input('enter username')
s1=input('enter password')
a=6
print("Available username is "+str(username(a)))
print("Available password is "+str(password(a)))
